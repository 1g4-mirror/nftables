nft-obj			+= cli.o
endif
nft-obj			+= rule.o
nft-obj			+= statement.o
nft-obj			+= datatype.o
nft-obj			+= expression.o
nft-obj			+= evaluate.o
nft-obj			+= proto.o
nft-obj			+= payload.o
nft-obj			+= exthdr.o
nft-obj			+= meta.o
nft-obj			+= ct.o
nft-obj			+= netlink.o
nft-obj			+= netlink_linearize.o
nft-obj			+= netlink_delinearize.o
nft-obj			+= segtree.o
nft-obj			+= rbtree.o
nft-obj			+= gmputil.o
nft-obj			+= utils.o
nft-obj			+= erec.o
nft-obj			+= mnl.o

nft-obj			+= parser_bison.o
nft-extra-clean-files	+= parser_bison.c parser_bison.h

nft-obj			+= scanner.o
nft-extra-clean-files	+= scanner.c scanner.h
